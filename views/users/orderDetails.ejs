            <div class="col-lg-8">
                <div class="row">
                    <div class="col-md-12">
                        <div class="cart-detail">
                            <span> <span>Order ID </span value=""><%= id.toString().substr(18, 24)  %></span>
                            <h2>Delivery Address</h2>
                            <ul>
                                <li>
                                    <h4><%= userData.name %></h4> <span></span>

                                   
                                        <ul>

                                            <li><span><%= userData.address[L-1].street %>, <%= userData.address[L-1].city %>, <%= userData.address[L-1].state %>, <%= userData.address[L-1].zipCode %></span> <span></span></li>
                                        </ul>
                                        
                                </li>
                                <li><span><%=userData.mobile %></span> </li>
                                <li><span><%=userData.email %></span> </li>
                            </ul>
                        </div>
                   </div>
                </div>
            </div>

            
				<div class="row row-pb-lg">
					<div class="col-md-12">
						<div class="product-name d-flex">
							<div class="one-forth text-left px-4">
								<span>Product Details</span>
							</div>
							<div class="one-eight text-center">
								<span>Quantity</span>
							</div>
							<div class="one-eight text-center">
								<span>price</span>
							</div>
							<div class="one-eight text-center">
								<span>Status</span>
							</div>
							<div class="one-eight text-center px-4">
								<span>payment methode</span>
							</div>
							<%  if(orderData.status == "Order Delivered"){%>

							<div class="one-eight text-center px-4">
								<span></span>
							</div>
							<% } %>

						</div>
                        <% for (let i = 0; i < orderData.item.length; i++) { %>

                        <div class="product-cart d-flex">
							<div class="one-forth">
								<div>
                                    <img style="height: 70px;width: 70px;" src="/images/<%= orderData.item[i].product.image[0]%>" alt="">

								</div>
								<div class="display-tc">
									<h3><%- orderData.item[i].product.productName %></h3>
								</div>
							</div>
							<div class="one-eight text-center">
								<div class="display-tc">
									<span class="price"><%- orderData.item[i].quantity %></span>
								</div>
							</div>
							<div class="one-eight text-center">
								<div class="display-tc">
									<span class="price"><%- orderData.item[i].total %></span>
								</div>
							</div>
							<div class="one-eight text-center">
								<div class="display-tc">
									<span class="price"><%- orderData.status %></span>
								</div>
							</div>
							<div class="one-eight text-center">
								<div class="display-tc">
									<span class="price"><%- orderData.paymentType %></span>
								</div>
							</div>
							
							
						<%  if(orderData.status == "Order Delivered"){%>
							<div class="one-eight text-center">
								<div class="display-tc">
									<a href="/feedback?productId=<%- orderData.item[i].product._id %>">Rate & Review</a>  
								</div>
							</div>
							<% } %>
						</div>
                        <% }  %>
					</div>
                  
                <div class="container">
                    <div class="row justify-content-md-center">
                        <% if (orderData.status == "Order Confirmed" || orderData.status == "Order shipped" ){ %>

                      <form action="/oredrCancel?id=<%= id  %>" method="post">
                      <div class="col-md-auto">
                       <a href="/oredrCancel?id=<%= id  %>"><button class="btn-danger">Cancel Order</button> </a>
                    </div>
                    </form>
                    <%
                } else if(orderData.status == "Order Delivered"){%>
                    <form action="/orderReturn?id=<%= id  %>" method="post">
                      <div class="col-md-auto">
                       <a href="/orderReturn?id=<%= id  %>"><button class="btn-danger">Return Order</button> </a>
                    </div>
                    </form>
                    <%
                           } else{%>
                            <%} %>
                    </div>
                </div>

				</div>







				<div class="row">
